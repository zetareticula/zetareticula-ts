{
  "name": "moe-inference-system",
  "version": "1.0.0",
  "description": "Mixture of Experts inference system with dynamic bit-depth optimization",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
